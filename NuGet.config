<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="SahajArtifacts" value="https://pkgs.dev.azure.com/Sahajinfotech/_packaging/62550f37-9592-43a0-bf02-42aa1ce13edc/nuget/v3/index.json" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <!-- send internal packages to your feed -->
    <packageSource key="SahajArtifacts">
