<script>
	var arrSamp = [];

	$(function () {
		// Initialize Select2 on the select element
		$('#tatseleled').select2();

		// When an option is selected
		$('#tatseleled').on('select2:select', function (e) {
			// Hide all elements with the 'hide' class
			$('.hide').hide();

			// Show the element matching the selected value's class
			$('.' + $(this).val()).show();

			// Get the selected value
			let selectedValue = $(this).val();

			// If "fixed" is selected
			if (selectedValue === 'fixed') {
				// Show the element with class 'fixed' (time input)
				$('.fixed').show();

				// Hide the element with class 'fiexhide' (div to hide when fixed is selected)
				$('.fiexhide').hide();
			} else {
				// Show the element with class 'fiexhide' for other dropdown values
				$('.fiexhide').show();
			}

			// Close the Select2 dropdown
			$(this).select2('close');
		});
	});

</script>
<script>
