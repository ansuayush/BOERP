@{
    ViewBag.Title = "Inventory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- page title   -->

<ul class="tablink">
    <li><a href="/MaterialManagement/Material/Inventory?auth=@ViewBag.Auth" class="nav-link  active">Stock Statement</a></li>
    <li><a href="/MaterialManagement/Material/StockLedger?auth=@ViewBag.Auth" class="nav-link">Stock Ledger</a></li>
    <li><a href="/MaterialManagement/Material/BindItemMasterList?auth=@ViewBag.Auth" class="nav-link">Item Master</a></li>

</ul>

<!--  -->
<!-- Open -->

<fourbox-widgets></fourbox-widgets>

<div class="pagetitle gap-bottom d-flex justify-content-between">
    <h5 class="title-two m-0 align-self-center">Inventory Report <img src="~/assets/images/icons/help/info-icon.png" alt="info icon" class="icon-sm" data-toggle="tooltip" title="This page shows list of all the products"></h5>
    <div class="align-self-center gap-10 d-flex">
        <a href="" class="btn border-r-8" data-toggle="tooltip" title="Download"><img src="~/assets/images/icons/help/filter/download-icon.svg" class="btn-icon m-0"></a> <a href="" class="btn border-r-8" data-toggle="tooltip" title="Export"><img src="~/assets/images/icons/help/filter/export-icon.svg" class="btn-icon m-0"></a>  <a href="" class="btn border-r-8" data-toggle="tooltip" title="Import"><img src="~/assets/images/icons/help/filter/import-icon.svg" class="btn-icon m-0"></a>
        <div class="btn border-r-8 dropdown">
            <button class="dropdown-toggle arrow-none" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="false" aria-expanded="false">
                <img src="~/assets/images/icons/help/filter/setting-icon.svg" class="single-icon"> Actions
            </button>
			<div class="dropdown-menu actiondropdown dropdown-menu-right  " aria-labelledby="dropdownMenuButton">
				@*<a class="dropdown-item d-flex gap-10" href="" data-toggle="modal" data-target="#updateitemstock">*@
				<a class="dropdown-item d-flex gap-10" href="/MaterialManagement/Material/StockAdjustment?auth=@ViewBag.Auth">
					<div class="actionicon align-self-center">
						<img src="~/assets/images/icons/actions/update-stock-icon.svg" class="icon-sm" alt="">
					</div>
					<div class="actionifno align-self-center">
						Manual Stock Adjustment
						<small class="d-block">Add or reduce item quantity in bulk</small>
					</div>
				</a>

				<a class="dropdown-item d-flex  gap-10" href="" data-toggle="modal" data-target="#stocktransfer">
					<div class="actionicon align-self-center">
						<img src="~/assets/images/icons/actions/stock-transfer-icon.svg" class="icon-sm" alt="">
					</div>
					<div class="actionifno align-self-center">
						Stock Transfer
						<small class="d-block">Transfer your stock between stores</small>
					</div>
				</a>

				<a class="dropdown-item d-flex  gap-10" href="" data-toggle="modal" data-target="#managecategory">
					<div class="actionicon align-self-center">
						<img src="~/assets/images/icons/actions/categories-icon.svg" class="icon-sm" alt="">
					</div>
					<div class="actionifno align-self-center">
						Item Categories
						<small class="d-block">Create multiple categories for your items</small>
					</div>
				</a>
				<a class="dropdown-item d-flex gap-10" href="" " data-toggle="modal" data-target="#managesubcategory">
					<div class="actionicon align-self-center">
						<img src="~/assets/images/icons/actions/subcategory-icon-icon.svg" class="icon-sm" alt="">
					</div>
					<div class="actionifno align-self-center">
						Sub Categories
						<small class="d-block">Create multiple sub categories for items</small>
					</div>
				</a>

			</div>
        </div>

    </div>
</div>

<div class="secondary-light-bg third-container">
    <div class="row table-filter">
        <div class="col-sm-2 form-group">
            <label for="role">From (Date) </label>
            <div class="input-group border-light border-r-8 cursor-pointer">
                <input type="text" class="datepicker form-control b-zero" placeholder="DD/MM/YYYY" value="15/02/2025">
                <div class="input-group-append  ">
                    <span class="input-group-text bg-white b-zero"><img src="../assets/images/icons/help/calendar-icon.svg" alt=""></span>
                </div>
            </div>
        </div>
        <div class="col-sm-2 form-group">
            <label for="role">To (Date) </label>
            <div class="input-group border-light border-r-8 cursor-pointer">
                <input type="text" class="datepicker form-control b-zero" placeholder="DD/MM/YYYY" value="15/02/2025">
                <div class="input-group-append  ">
                    <span class="input-group-text bg-white b-zero"><img src="../assets/images/icons/help/calendar-icon.svg" alt=""></span>
                </div>
            </div>
        </div>

        <div class="col-sm-2 form-group">
            <label for="role">Warehouse </label>
            <select class="form-control applyselect">
                <option>All</option>
                <option>Abstergo Ltd. </option>
                <option>Biffco Enterprises Ltd. </option>
                <option>Binford Ltd.</option>
                <option>Barone LLC.</option>
            </select>
        </div>
        <div class="col-sm-2 form-group">
            <label for="role">Status </label>
            <select class="form-control applyselect">

                <option>All</option>
                <option>Pending</option>
                <option>Approved</option>
                <option>Rejected</option>
            </select>
        </div>
        <div class="col-sm-2 form-group">
            <label for="role">Category </label>
            <select class="form-control applyselect">

                <option>All</option>
                <option>WH 1</option>
                <option>WH 2</option>
                <option>WH 3</option>
            </select>
        </div>


        <div class="col-sm-2 form-group">
            <label for="role">Show/Hide Columns 1</label>
            <select class="form-control border-r-8 multiple-checkboxes" multiple="multiple">
